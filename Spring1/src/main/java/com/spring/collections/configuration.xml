<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.spring.collections.Collec" name="coll">
	
		<property name="name" value="SHUBHAM TAWARE" />

		<property name="phone">
			<list>
				<value>11</value>
				<value>12</value>
				<value>3133</value>
				<value>23231</value>
			</list>
		</property>

		<property name="address">
			<set>
				<value>Pune</value>
				<value>Mumbai</value>
				<value>Delhi</value>
				<value>CA</value>
			</set>
		</property>

		<property name="courses">
			<map>
				<entry key="first" value="3" />
				<entry key="second" value="5" />
				<entry key="third" value="6" />
			</map>
		</property>
	</bean>
</beans>
